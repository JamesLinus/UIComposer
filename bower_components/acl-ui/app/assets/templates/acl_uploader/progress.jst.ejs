<div class="acl-uploader__progress">
  <div class="acl-uploader__progress-bar"></div>
  <i class="acl-i-check"></i>
  <i class="acl-i-exclamation-circle"></i>
  <div class="acl-throbber"></div>

  <span class="acl-uploader__filename"><%= filename %></span>
  <span class="acl-uploader__error-message"></span>

  <span class="acl-uploader__meta">
    <span class="acl-uploader__info"></span>
    <span class="acl-uploader__processing"><%= I18n.t("acl_uploader.processing") %></span>
  </span>
</div>
